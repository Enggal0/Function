#include <iostream>

int faktorial(int n) {
    if (n <= 1) {
        return 1;
    } else {
        return n * faktorial(n - 1);
    }
}

int main() {
    int angka = 5;
    int hasil = faktorial(angka);
    std::cout << "Faktorial dari " << angka << " adalah: " << hasil << std::endl;
    return 0;
}
